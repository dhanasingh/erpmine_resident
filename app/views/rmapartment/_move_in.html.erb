<% unless leadObj.blank? %>
	
	<%#= link_to l(:button_move_in), url_for(:controller => "rmapartment", :action => "movein", :tab => "rmapartment", :lead_id => leadObj.blank? ? nil : leadObj.id, :res_action => "MI"),  :class=> "buttons" %>
	<%
		wktime_helper = Object.new.extend(WktimeHelper)
		host_with_subdir = wktime_helper.getHostAndDir(request)
	%>
	
	<input type="button" value="<%= l(:button_move_in) %>" onclick="window.location.href='<%= url_for(:controller => "rmapartment", :action => "movein", :host => host_with_subdir, :only_path => true, :tab => "rmapartment", :lead_id => leadObj.blank? ? nil : leadObj.id, :res_action => "MI") %>'" />
	
<% end %>