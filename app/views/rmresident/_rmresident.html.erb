			<%= link_to l(:label_new_res_service), url_for(controller: 'rmresident', action: 'newresidentservice', rm_resident_id: params[:rm_resident_id], service_type: 'RS'), class: 'icon icon-time-add' %>
	</div>	
	<%= render partial: "rmresident/resident_service_list", locals: { resservices: @resObj.blank? ? nil : @resObj.resident_services.includes(:issue).where(issues: { tracker_id: [Setting.plugin_erpmine_resident['rm_service_tracker']] }), serviceType: "RS" } %>
